{
    "appId": "org.desolatesea.astara",
    "appName": "Astara",
    "webDir": "dist",
    "server": {
        "androidScheme": "https"
    },
    "android": {
        "allowMixedContent": true,
        "webContentsDebuggingEnabled": false,
        "adjustMarginsForEdgeToEdge": "force"
    },
    "plugins": {
        "SystemBars": {
            "style": "DARK",
            "hidden": true,
            "insetsHandling": "css"
        }
    }
}